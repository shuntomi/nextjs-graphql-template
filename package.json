{
  "name": "with-typescript-graphql",
  "version": "0.1.0",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "dev:codegen": "graphql-codegen --config codegen.yml --watch",
    "lint": "eslint ./src --ext ts --ext tsx --ext js",
    "lint-fix": "yarn lint --fix && yarn format --loglevel error",
    "format": "prettier --write .",
    "build": "next build",
    "test": "jest",
    "typecheck": "tsc",
    "start": "next start",
    "spellcheck": "cspell \"**/*.{css,scss,js,jsx,ts,tsx,md,yaml,yml,json}\"",
    "prepare": "husky install",
    "generate": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@graphql-tools/load-files": "6.0.18",
    "@graphql-tools/merge": "6.0.18",
    "@graphql-tools/schema": "6.0.18",
    "apollo-server-micro": "^2.16.1",
    "graphql": "15.3.0",
    "next": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.34.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/plugin-helpers": "^1.17.8",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@graphql-codegen/typescript-react-apollo": "2.2.5",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "16.9.3",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "26.3.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "cspell": "^5.6.3",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "graphql-tag": "2.11.0",
    "husky": "^6.0.0",
    "hygen": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.4.0",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.15.2",
    "prettier": "^2.3.1",
    "react-test-renderer": "^16.9.0",
